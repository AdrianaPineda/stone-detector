function [ img ] = normalizeColors( img )
%UNTITLED Summary of this function goes here
%   Detailed explanation goes here

    % Background color
    % background color is ieasy: its in the corners always so just
    % get the color of a corner
    backgroundColor = img(5,5);

    % Bone color is trickier to  
    boneX = uint16(sizeX(img)/2);
    boneY = uint16(sizeY(img)*0.45); 
    boneColor = img(boneY, boneX);
    
    organColor = -1;
    
    val_preceding = img(boneY,boneX);
    for x=boneX+1:(boneX-1)*2
        val = img(boneY,x);
        if abs(val-val_preceding) > 0
          organColor = val;
          break;
        end
    end
    
    img = [backgroundColor boneColor organColor];
            
    
end

